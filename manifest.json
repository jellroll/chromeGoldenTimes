{
"manifest_version": 2,
    "name": "Golden Times",
    "description": "Replace politician names with names of Golden Girls characters",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],

    "browser_action": {
      "default_icon": "sun_square_48.png"
    }


}